<% var methods = ['get','update','delete'] %>

<% types.forEach((type, index) => {
%>export const <%= type %> = '<%= type %>';
<% }) %><% type.forEach((t, index) => {
%>export const <%= t %> = '<%= t %>';
<% }) %>

<% methods.forEach((method, index) => {
%>const <%= method %><%=Plural%>ActionTypes = {
  request: <%= types[index] %>,
  success: <%= types[index] %>,
  fail: <%= types[index] %>,
}
<% }) %>
<% methods.forEach((method, index) => {
%>const <%= method %><%=Name%>ActionTypes = {
  request: <%= type[index] %>,
  success: <%= type[index] %>,
  fail: <%= type[index] %>,
}
<% }) %>

const initialState = {
  data: [],
  sort: 'ASC',
  orderby: 'id',
  search: '',
  mustLoad: true,
  error: {},
  isFetching: false
};

export default function (state = initialState, action) {
  switch (action.type) {
    <% types.forEach((type, index) => { %>
      case <%= type %>:
        return {
          ...state,
        }<% }) %><% type.forEach((t, index) => { %>
      case <%= t %>:
        return {
          ...state,
        }<% }) %>
    default: return state
  }
}


